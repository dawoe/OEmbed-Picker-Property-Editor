{
  "$schema": "../../umbraco-package-schema.json",
  "name": "OEmbed Property Editor",
  "version": "17.0.1",
  "extensions": [
    {
      "type": "propertyEditorUi",
      "alias": "Dawoe.OEmbedPickerPropertyEditor.Ui",
      "name": "OEmbed Picker",
      "element": "/App_Plugins/Dawoe.OEmbedPickerPropertyEditor/client.js",
      "elementName": "dawaoe-oembed-picker",
      "meta": {
        "label": "OEmbed Picker",
        "icon": "icon-tv",
        "group": "common",
        "propertyEditorSchemaAlias": "Dawoe.OEmbedPickerPropertyEditor"
      }
    },
    {
      "type": "propertyEditorSchema",
      "alias": "Dawoe.OEmbedPickerPropertyEditor",
      "name": "OEmbed Picker",
      "meta": {
        "defaultPropertyEditorUiAlias": "Dawoe.OEmbedPickerPropertyEditor.Ui",
        "settings": {
          "properties": [
            {
              "alias": "allowMultiple",
              "label": "Allow multiple",
              "description": "Allow adding multiple items",
              "propertyEditorUiAlias": "Umb.PropertyEditorUi.Toggle"
            }
          ]
        }
      }
    }
  ]
}
